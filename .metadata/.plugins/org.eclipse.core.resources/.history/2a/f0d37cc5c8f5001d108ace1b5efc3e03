		try {
			Connection con = ConnectionFactory.getConnection();
			 PreparedStatement= con.prepareStatement("insert into contatos (nome, email, endereco)) values (?, ?, ?,)");
			
			
			
		} catch (SQLException e) {
			
			e.printStackTrace();
		}
	

	}

}
